{
    "extends": ["airbnb"],
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "env": {
      "browser": true,
      "node": true,
      "jest": true
    },
    "rules": {
      "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
      "import/extensions": ["error", "ignorePackages", {
        "js": "never",
        "mjs": "never",
        "jsx": "never"
      }]
    }
  }
  